if(!require(pacman)) install.packages("pacman")
library(pacman)
pacman::p_load(dplyr, car, rstatix, lmtest, ggpubr,
QuantPsyc, psych, scatterplot3d)
dados <- read.csv2('NDVI.csv', stringsAsFactors = T) # Carregamento do arquivo csv
View(dados)                                 # Visualização dos dados em janela separada
glimpse(dados)
mod <- lm(NDVI ~ Rn + Gn + Bn + H + S + V, dados)
par(mfrow=c(2,2))
plot(mod)
par(mfrow=c(1,1))
shapiro.test(mod$residuals)
summary(rstandard(mod))
durbinWatsonTest(mod)
bptest(mod)
pairs.panels(dados)
vif(mod)
mod2 <- lm(NDVI ~ Rn + Gn + Bn + H + S + V, dados)
summary(mod)
summary(mod2)
lm.beta(mod)
lm.beta(mod2)
confint(mod)
confint(mod2)
AIC(mod, mod2)
BIC(mod, mod2)
anova(mod, mod2)
graph <- scatterplot3d(dados$Notas ~ dados$Tempo_Rev + dados$Tempo_Sono,
pch = 16, angle = 30, color = "steelblue", box = FALSE,
xlab="Tempo de revisão", ylab="Tempo de sono", zlab="Notas")
graph$plane3d(mod, col="black", draw_polygon = TRUE)
graph <- scatterplot3d(dados$NDVI ~ dados$Rn + dados$Gn + dados$Bn + dados$H + dados$S + dados$V,
pch = 16, angle = 30, color = "steelblue", box = FALSE,
xlab="Tempo de revisão", ylab="Tempo de sono", zlab="Notas")
graph$plane3d(mod, col="black", draw_polygon = TRUE)
# Passo 5: Gráfico de dispersão
graph <- scatterplot3d(dados$NDVI ~ dados$Rn + dados$Gn,
pch = 16, angle = 30, color = "steelblue", box = FALSE,
xlab="Tempo de revisão", ylab="Tempo de sono", zlab="Notas")
pacman::p_load(MASS)
mod.inicial <- lm(NDVI ~ Rn + Gn+ Bn + H + S + V, data = dados)
mod.simples <- lm(NDVI ~ 1, data = dados)
stepAIC(mod.inicial, scope = list(upper = mod.inicial,
lower = mod.simples), direction = "backward")
